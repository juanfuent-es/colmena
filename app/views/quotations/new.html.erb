<% provide(:title, "Nueva quotation") %>

<header class="content-header">
  <div class="container-fluid">
    <div class="row mb-2">
      <div class="col-sm-6">
        <h1><%= "#{t(:new)} #{t(:quotation)}" %></h1>
      </div>
      <div class="col-sm-6">
        <ol class="breadcrumb float-sm-right mt-2">
          <li class="breadcrumb-item">
            <%= link_to t(:quotations), admin_quotations_path %>
          </li>
          <li class="breadcrumb-item active">
            <%= "#{t(:new)} #{t(:quotation)}" %>
          </li>
        </ol>
      </div>
    </div>
  </div><!-- /.container-fluid -->
</header>

<%= render 'form', quotation: @quotation %>